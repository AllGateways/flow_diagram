#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules/webpack/bin/webpack.js/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules/webpack/bin/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules/webpack/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/webpack/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/node_modules:/Applications/Splunk/etc/apps/flowchart_app/node_modules:/Applications/Splunk/etc/apps/node_modules:/Applications/Splunk/etc/node_modules:/Applications/Splunk/node_modules:/Applications/node_modules:/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/webpack/bin/webpack.js/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/webpack/bin/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/webpack/node_modules" "$basedir/node"  "$basedir/../webpack/bin/webpack.js" "$@"
  ret=$?
else 
  NODE_PATH="/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules/webpack/bin/webpack.js/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules/webpack/bin/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules/webpack/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/webpack/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/node_modules:/Applications/Splunk/etc/apps/flowchart_app/node_modules:/Applications/Splunk/etc/apps/node_modules:/Applications/Splunk/etc/node_modules:/Applications/Splunk/node_modules:/Applications/node_modules:/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/webpack/bin/webpack.js/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/webpack/bin/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/webpack/node_modules" node  "$basedir/../webpack/bin/webpack.js" "$@"
