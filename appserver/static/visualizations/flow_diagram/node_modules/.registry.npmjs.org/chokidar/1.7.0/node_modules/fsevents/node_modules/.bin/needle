#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules/needle/bin/needle/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules/needle/bin/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules/needle/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/fsevents/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/node_modules:/Applications/Splunk/etc/apps/flowchart_app/node_modules:/Applications/Splunk/etc/apps/node_modules:/Applications/Splunk/etc/node_modules:/Applications/Splunk/node_modules:/Applications/node_modules:/node_modules" "$basedir/node"  "$basedir/../needle/bin/needle" "$@"
  ret=$?
else 
  NODE_PATH="/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules/needle/bin/needle/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules/needle/bin/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules/needle/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/fsevents/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules/.registry.npmjs.org/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/flowchart/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/visualizations/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/static/node_modules:/Applications/Splunk/etc/apps/flowchart_app/appserver/node_modules:/Applications/Splunk/etc/apps/flowchart_app/node_modules:/Applications/Splunk/etc/apps/node_modules:/Applications/Splunk/etc/node_modules:/Applications/Splunk/node_modules:/Applications/node_modules:/node_modules" node  "$basedir/../needle/bin/needle" "$@"
